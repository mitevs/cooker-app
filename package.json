    "koa-html-minifier": "^1.0.1",
    "koa-mount": "^4.0.0",
    "koa-pino-logger": "^2.1.3",
    "koa-router": "^7.4.0",
    "koa-session": "^5.12.3",
    "koa-static": "^5.0.0",
    "node-fetch": "^2.6.0",
    "polished": "^3.4.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-loadable": "^5.5.0",
    "react-router-config": "^5.0.1",
