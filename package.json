{
  "name": "cooker-app",
  "version": "1.0.0",
  "scripts": {
    "start": "node dist/server",
    "build": "npm-run-all --parallel build:client build:server",
    "build:client": "cross-env BABEL_ENV=client NODE_ENV=production npx webpack --config src/client/webpack.config.js",
    "build:client:dev": "cross-env BABEL_ENV=client NODE_ENV=development npx webpack --config src/client/webpack.config.js",
    "build:server": "cross-env BABEL_ENV=server NODE_ENV=production npx webpack --config src/server/webpack.config.js",
    "dev:client": "cross-env BABEL_ENV=client NODE_ENV=development HMR=true npx webpack-dev-server --config src/client/webpack.config.js",
    "dev:server": "cross-env BABEL_ENV=server NODE_ENV=development HMR=true npx webpack --watch --config src/server/webpack.config.js",
    "dev": "cross-env NODE_ENV=development HMR=true npm-run-all --parallel dev:server dev:client",
    "analyse": "source-map-explorer dist/public/*.js",
    "sb": "start-storybook -p 9001 -c config/storybook",
    "release": "npx standard-version"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.6.0",
    "@storybook/addon-actions": "^5.2.4",
    "@storybook/addon-knobs": "^5.2.4",
    "@storybook/addon-viewport": "^5.2.4",
    "@storybook/react": "^5.2.4",
    "@svgr/webpack": "^4.3.3",
    "@types/koa": "^2.0.49",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-ejs": "^4.2.0",
    "@types/koa-html-minifier": "^1.0.1",
    "@types/koa-mount": "^4.0.0",
    "@types/koa-pino-logger": "^2.1.6",
    "@types/koa-router": "^7.0.42",
    "@types/koa-session": "^5.10.1",
    "@types/koa-static": "^4.0.1",
    "@types/node-fetch": "^2.5.0",
    "@types/react": "^16.9.6",
    "@types/react-dom": "^16.9.2",
    "@types/react-loadable": "^5.5.2",
    "@types/react-router-config": "^5.0.0",
    "@types/react-router-dom": "^5.1.0",
    "@types/styled-components": "^4.1.19",
    "@types/webpack-env": "^1.14.1",
    "apollo-client": "^2.6.4",
    "babel-loader": "^8.0.6",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-polyfill": "^6.26.0",
    "copy-webpack-plugin": "^5.0.4",
    "cross-env": "^6.0.3",
    "html-webpack-plugin": "^3.2.0",
    "npm-run-all": "^4.1.5",
    "pino-pretty": "^3.2.2",
    "prettier": "^1.18.2",
    "react-docgen-typescript-loader": "^3.3.0",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "source-map-explorer": "^2.0.1",
    "standard-version": "^7.0.1",
    "start-server-webpack-plugin": "^2.2.5",
    "styled-components-modifiers": "^1.2.5",
    "typescript": "^3.6.2",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.8",
    "webpack-dev-server": "^3.8.0",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.0",
